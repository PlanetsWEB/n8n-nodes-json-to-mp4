{
  "name": "n8n-nodes-json-to-mp4",
  "version": "1.0.0",
  "description": "Community node para usar a API JsonToMp4 no n8n.",
  "author": "Grupo PlanetsWEB",
  "license": "MIT",
  "keywords": [
    "n8n",
    "workflow",
    "automation",
    "json",
    "video",
    "mp4"
  ],
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "start": "nodemon --exec \"node cmd-start.js\"",
    "preinstall": "npx only-allow npm",
    "build": "tsc && gulp build:icons",
    "watch": "tsc --watch",
    "format": "prettier ./src --write",
    "lint": "eslint",
    "lintfix": "eslint --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "n8n-core": "^1.91.0",
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-json-to-mp4": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n": "^1.64.3",
    "n8n-node-dev": "^1.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "files": [
    "src"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "src/credentials/JsonToMp4Api.credentials.js"
    ],
    "nodes": [
      "src/nodes/JsonToMp4.node.js"
    ]
  }
}
